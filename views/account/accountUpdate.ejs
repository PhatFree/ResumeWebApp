<%- include("../includes/header.ejs", {title:"Update" + result.email}); %>

<h1>Update <%= result.email%></h1>
<form action="/account/update" method="get">
    <input type="hidden" id="account_id" name="account_id" value="<%= result[0].account_id %>">
    Account email: <input id="email" name="email" type="text" value="<%= result[0].email %>"/>
    <br />
    First Name: <input id="first_name" name="first_name" type="text" value="<%= result[0].first_name %>"/>
    <br />
    Last Name: <input id="last_name" name="last_name" type="text" value="<%= result[0].last_name %>"/>
    <br />

    <input type="submit" />

</form>
<a href="/account/all"><< back</a>


<%- include("../includes/footer.ejs"); %>